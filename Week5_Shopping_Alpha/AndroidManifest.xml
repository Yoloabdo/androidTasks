<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.week5_shopping_alpha"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.week5_shopping_alpha.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.week5_shopping_alpha.BudgetAcitivity"
            android:label="@string/title_activity_budget_acitivity" >
        </activity>
        <activity
            android:name="com.example.week5_shopping_alpha.ProductsActivity"
            android:label="@string/title_activity_products" >
        </activity>
        <activity
            android:name="com.example.week5_shopping_alpha.ReportActivity"
            android:label="@string/title_activity_report" >
        </activity>
        <activity
            android:name="com.example.week5_shopping_alpha.TestReport"
            android:label="@string/title_activity_test_report" >
        </activity>
        <activity
            android:name="com.example.week5_shopping_alpha.TestFest"
            android:label="@string/title_activity_test_fest" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
